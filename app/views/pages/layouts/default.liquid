---
title: Default layout
is_layout: true
---
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="icon" type="image/png" href="{{ 'favicon.png' | theme_image_url }}" />
    {% seo %}
    {{ 'bundle.css' | stylesheet_tag }}
    {{ 'bundle.js' | javascript_tag }}
  </head>
  <body>
    {% include 'preloader' %}
    {% block 'main' %}
    {% endblock %}
    {% include 'modal' %}
    {% section 'footer', placement: 'bottom' %}
    {% include 'backtotop' %}
    {% include 'sidebar' %}
    {% if wagon %}<script src="http://localhost:35729/livereload.js"></script>{% endif %}
  </body>
</html>